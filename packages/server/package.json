  "module": "dist.esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:cjs": "tsc -b",
    "build:esm": "tsc -b tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "start": "npm run build && node dist/main",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "fix:firebase:win": "cd ../firebase && npm run build && cd ../server && rimraf node_modules/@banta/firebase && cpy ../firebase node_modules/@banta && rimraf node_modules/@banta/firebase/node_modules",
    "fix:firebase:mac": "cd ../firebase && npm run build && cd ../server && rimraf node_modules/@banta/firebase && cp -Rf ../firebase node_modules/@banta && rimraf node_modules/@banta/firebase/node_modules"
  },
